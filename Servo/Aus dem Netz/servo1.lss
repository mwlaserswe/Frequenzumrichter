static inline unsigned char lcd_waitbusy(void)
/* loops while lcd is busy, reads address counter */
{
    register unsigned char c;

    while ( (c=lcd_read(0)) & (1<<LCD_BUSY)) {}

    return (c);  // return address counter
}


